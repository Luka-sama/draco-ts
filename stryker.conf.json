{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": [
    "html",
    "clear-text",
    "progress-append-only"
  ],
  "testRunner": "command",
  "coverageAnalysis": "perTest",
  "tempDirName": "stryker-tmp",
  "checkers": ["typescript"],
  "mutate": ["src/**/*.ts", "!src/**/*.entity.ts", "!src/**/*.test.ts"],
  "jest": {
    "configFile": "dist/jest.config.js"
  },
  "htmlReporter": {
    "baseDir": "stryker-report"
  }
}